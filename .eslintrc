{
    "env": {
        "node": true,
        "mocha": true
    },
    "plugins": [
        "jsdoc"
    ],
    "parserOptions": {
        "ecmaVersion": 2023,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "extends": "eslint-config-topcoder/nodejs",
    "rules": {
        "max-len": [
            "error",
            160
        ],
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 0,
        "comma-dangle": [
            "error",
            {
                "arrays": "never",
                "objects": "never",
                "imports": "never",
                "exports": "never",
                "functions": "ignore"
            }
        ],
        "import/no-unresolved": 0,
        "max-params": 0,
        "max-lines": 0,
        "max-statements": 0,
        "valid-jsdoc": 0
    }
}
